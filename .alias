# kubectl alias
alias kcr='kd create'
alias kcoc='kubectl config current-context'
alias kcog='kubectl config get-contexts'
alias kcou='kubectl config use-context'
alias klo='kubectl logs'
alias klof='kubectl logs -f'
alias kgt='kubectl get'
alias kgtp='kubectl get pods'
alias kgtd='kubectl get deployments'
alias kdsp='kubectl describe pod'
alias kdsd='kubectl describe deployment'
alias kdl='kubectl delete'
alias kdld='kubectl delete deployment'
alias kdlp='kubectl delete pod'
alias kex='kubectl exec -ti'
alias kpf='kubectl port-forward'
